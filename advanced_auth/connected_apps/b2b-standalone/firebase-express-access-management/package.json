{
  "name": "@mcp-examples/firebase-express-access-management",
  "description": "Express.js API for managing access requests with Firebase authentication",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "build:server": "tsc --project tsconfig.server.json",
    "build:frontend": "vite build",
    "build": "concurrently \"npm run build:server\" \"npm run build:frontend\"",
    "start:server": "node dist/server/server.js",
    "start:frontend": "vite preview --port 3001",
    "start": "concurrently \"npm run start:server\" \"npm run start:frontend\"",
    "dev:server": "NODE_ENV=tsxwatch tsx watch server/server.ts",
    "dev:frontend": "vite",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
    "lint:server": "eslint server --ext ts --report-unused-disable-directives --max-warnings 0",
    "lint:frontend": "eslint frontend --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint": "concurrently \"npm run lint:server\" \"npm run lint:frontend\"",
    "prettier:check": "prettier --check frontend scripts server",
    "prettier:fix": "prettier --write frontend scripts server",
    "type-check": "tsc --noEmit",
    "setup:firebase": "tsx scripts/setup.ts --firebase",
    "setup:stytch": "tsx scripts/setup.ts --stytch",
    "setup:localhost": "tsx scripts/setup.ts --localhost",
    "setup:all": "tsx scripts/setup.ts --all",
    "setup": "tsx scripts/setup.ts --all"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@stytch/react": "^19.10.0",
    "@stytch/vanilla-js": "^5.34.0",
    "axios": "^1.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "firebase": "^10.7.0",
    "firebase-admin": "^11.11.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "stytch": "^12.32.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^9.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "inquirer": "^12.9.4",
    "postcss": "^8.4.31",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.3.5",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.6.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^5.4.20"
  },
  "keywords": [
    "express",
    "firebase",
    "access-management",
    "api"
  ],
  "author": "",
  "license": "MIT"
}
