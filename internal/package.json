{
  "name": "@stytch-all-examples/internal",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "sideEffects": [
    "dist/styles.css",
    "src/index.ts"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./components/ui/*": "./components/ui/*",
    "./lib/*": "./lib/*",
    "./dist/styles.css": "./dist/styles.css",
    "./dist/fonts/*": "./dist/fonts/*"
  },
  "scripts": {
    "build": "npm run build:css && npm run build:fonts",
    "build:css": "tailwindcss -i ./components/styles.css -o ./dist/styles.css --config ./tailwind.config.js",
    "build:fonts": "mkdir -p dist/fonts && cp assets/fonts/*.woff2 dist/fonts/ 2>/dev/null || cp public/fonts/*.woff2 dist/fonts/ 2>/dev/null || true",
    "dev": "npm run dev:css && npm run build:fonts",
    "dev:css": "tailwindcss -i ./components/styles.css -o ./dist/styles.css --config ./tailwind.config.js --watch",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.534.0",
    "next-themes": "^0.4.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.6"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.0.0"
  }
}
